RewriteEngine On

# Если файл или директория существует, не перенаправляем
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d

# Перенаправляем все запросы на index.php с параметром route
# QSA - сохраняет существующие query параметры
# L - последнее правило (не продолжаем обработку)
RewriteRule ^(.*)$ index.php?route=$1 [QSA,L]